<UserControl x:Class="WpfApp2.HistoryPage"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             Height="300" Width="400">
    <Grid>
        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
            <TextBlock Text="Data history management" FontSize="20" FontWeight="Bold" Margin="0,10" TextAlignment="Center"/>
            <ListBox x:Name="HistoryList" Width="300" Height="200" Margin="0,10"/>
        </StackPanel>
    </Grid>
</UserControl>
